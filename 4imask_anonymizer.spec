binaries += collect_dynamic_libs('openvino')
hiddenimports += collect_submodules('openvino')


a = Analysis(
    ['4imask_anonymizer.py'],
    pathex=[],
    binaries=binaries,
    datas=datas,
    hiddenimports=hiddenimports,
    hookspath=[],
    hooksconfig={},
    runtime_hooks=[],
    excludes=[],
    noarchive=False,
    optimize=0,
)
pyz = PYZ(a.pure)
