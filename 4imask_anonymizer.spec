    'res/icons/splash.gif',
    binaries=a.binaries,
    datas=a.datas,
    text_pos=None,
    text_size=12,
    minify_script=True,
    always_on_top=False,
)

exe = EXE(
    pyz,
    a.scripts,
    splash,
    [],
    exclude_binaries=True,
    name='4imask_anonymizer',
    debug=False,
    bootloader_ignore_signals=False,
